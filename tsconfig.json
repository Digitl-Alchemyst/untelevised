{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noUnusedLocals": true,
    "jsx": "preserve",
    "incremental": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "plugins": [
      {
        "name": "next",
      },
    ],
    "rootDir": "./",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/a/*": ["./src/app/*"],
      "@/c/*": ["./src/components/*"],
      "@/h/*": ["./src/hooks/*"],
      "@/l/*": ["./src/lib/*"],
      "@/s/*": ["./src/store/*"],
      "@/u/*": ["./src/util/*"],
      "#/*": ["./*"],
      "$/*": ["./schemas/*"],
      "matrix-js-sdk": ["node_modules/@types/matrix-js-sdk/index.d.ts"],
      "react": ["node_modules/react"],
    },
    "outDir": "./out",
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts",
    "types.d.ts",
    "src/app/studio/[[...index]]/page.tsx",
    "src/loaders/resize-gif-loader.js",
    "build/types/**/*.ts",
    "schemas/layout.tsx",
  ],
  "exclude": ["node_modules"],
}
